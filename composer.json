{
  "name": "mediadevs/validator",
  "description": "A lightweigth end extendable PHP validation package.",
  "license": "MIT",
  "authors": [
    {
      "name": "Mike van Diepen",
      "email": "contact@mediadevs.nl",
      "homepage": "https://mediadevs.nl/",
      "role": "Developer"
    }
  ],
  "version": "1.0.0",
  "autoload": {
    "psr-4": {
        "Mediadevs\\Validator\\": "src/"
    }
  },
  "require": {
    "php": "^7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-fileinfo": "*",
    "mediadevs/configuration": "1.0.1",
    "mediadevs/response-handler": "1.0.0"
  },
    "require-dev": {
        "phpstan/phpstan": "^0.11.0",
        "phpunit/phpunit": "7",
        "php-coveralls/php-coveralls": "^2.0",
        "squizlabs/php_codesniffer": "3.*",
        "nunomaduro/phpinsights": "^1.11.1",
        "require seferov/typhp": "^0.2.0"
    }
}
